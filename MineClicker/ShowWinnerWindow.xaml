<Window x:Class="MineClicker.ShowWinnerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MineClicker"
        xmlns:p = "clr-namespace:MineClicker.Properties"
        mc:Ignorable="d"
        Title="{x:Static p:Resources.TitleWinnerWindow}" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Sourse_Imagen/TexturaFondoJuego.jpg"/>
        </Grid.Background>
        <Label Content="{x:Static p:Resources.WinnerMessage}"  HorizontalAlignment="Stretch" Margin="196,120,160.333,0" VerticalAlignment="Top" Foreground="#FFDCE4DA" FontFamily="Bold" FontSize="20" FontWeight="Bold" Height="33"/>
        <Label Content="Player" x:Name ="WinnerName"  HorizontalAlignment="Stretch" Margin="196,188,184.333,0" VerticalAlignment="Top" HorizontalContentAlignment="Center" Foreground="#FFDCE4DA" FontFamily="Bold" FontSize="20" FontWeight="Bold" Height="33"/>
        <Button Content="{x:Static p:Resources.BackButton}" HorizontalAlignment="Center" Margin="56,356,0,0" VerticalAlignment="Top" Width="114" Height="37" FontSize="18" FontWeight="Bold" />


    </Grid>
</Window>
